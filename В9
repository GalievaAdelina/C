```cpp
#include <iostream>
#include <cmath>

using namespace std;

int main() {
    double a, b;
    cout << "Введите два числа: ";
    cin >> a >> b;
    double geometric_mean = sqrt(a * b);
    cout << "Среднее геометрическое чисел " << a << " и " << b << " равно " << geometric_mean << endl;
    return 0;
}
```

