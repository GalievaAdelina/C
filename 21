a)

#include <iostream>
using namespace std;

int main() {
    float x, y, result;
    cout << "Введите значения x и y: ";
    cin >> x >> y;
    result = 2 - pow(x, 3) - 2.5 * x * y + 1.78 * pow(x, 2) - 12.5 * y + 1;
    cout << "Значение функции равно " << result << endl;
    return 0;
}

б)

#include <iostream>
using namespace std;

int main() {
    float a, b, x;
    cout << "Введите значения a и b: ";
    cin >> a >> b;
    x = 3.56 * pow(a + b, 3) - 5.8 * pow(b, 2) + 3.8 * a - 1.5;
    cout << "Значение функции равно " << x << endl;
    return 0;
}
