```cpp
#include <iostream>
using namespace std;

int main() {
  int a, b;
  cout << "Введите первое число: ";
  cin >> a;
  cout << "Введите второе число: ";
  cin >> b;
  
  cout << "До обмена числа: a = " << a << ", b = " << b << endl;
  
  int temp = a;
  a = b;
  b = temp;
  
  cout << "После обмена числа: a = " << a << ", b = " << b << endl;
  
  return 0;
}
```
